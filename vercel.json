{
  "version": 2,
  "buildCommand": "pip install -r clearpic-backend/requirements.txt",
  "outputDirectory": "clearpic-backend",
  "builds": [
    {
      "src": "clearpic-backend/main.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.9"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/clearpic-backend/main.py"
    },
    {
      "src": "/(.*)",
      "dest": "/clearpic-backend/main.py"
    }
  ],
  "env": {
    "PYTHONPATH": "clearpic-backend"
  }
}
